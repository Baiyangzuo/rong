extends layout

block append css
    link(rel="stylesheet", href="/css/libs/remodal.css")
    link(rel="stylesheet", href="/css/libs/remodal-default-theme.css")

block append js
    script(src="/js/libs/cookie.js")
    script(src='/js/libs/remodal.js')

block page
    include app/topbar
    include app/header
    include app/sbanner
    if(isMobile)
        include app/formui2
    else
        include app/formui
    include app/footer
    include app/succ
    include app/fail

block footerjs
    script(src="/v1/js/pages/apply.js")
    
